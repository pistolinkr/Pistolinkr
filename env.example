# Supabase 설정
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
SUPABASE_URL=https://your-project-id.supabase.co

# Edge Config 설정 (선택사항)
EDGE_CONFIG_ID=your-edge-config-id

# 기존 Vercel Postgres 설정 (선택사항)
POSTGRES_URL=postgres://username:password@host:port/database
POSTGRES_USER=username
POSTGRES_HOST=host
POSTGRES_PASSWORD=password
POSTGRES_DATABASE=database
POSTGRES_URL_NON_POOLING=postgres://username:password@host:5432/database
POSTGRES_PRISMA_URL=postgres://username:password@host:6543/database?sslmode=require&pgbouncer=true

# Supabase JWT 설정 (선택사항)
SUPABASE_JWT_SECRET=your-jwt-secret
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# 인증 비밀번호 (서버 사이드에서만 사용)
ADMIN_PASSWORD=your-admin-password-here
USER_PASSWORD=your-user-password-here

# EmailJS 설정
EMAILJS_PUBLIC_KEY=your_emailjs_public_key_here
EMAILJS_SERVICE_ID=your_emailjs_service_id_here
EMAILJS_TEMPLATE_ID=your_emailjs_template_id_here 